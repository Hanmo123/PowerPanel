    "vh-check": "^2.0.5",
    "vue": "^3.2.41",
    "vue-clipboard3": "^2.0.0",
    "vue-router": "^4.1.5",
    "xterm": "4.19.0",
    "xterm-addon-fit": "^0.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^16.11.68",
